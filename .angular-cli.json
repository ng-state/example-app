{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "example-app"
  },
  "apps": [
    {
      "root": "./",
      "outDir": "./dist",
      "assets": [
        "./src/assets",
        "./src/favicon.ico"
      ],
      "index": "./src/index.html",
      "main": "./src/main.ts",
      "test": "test.ts",
      "tsconfig": "./src/tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "./node_modules/bootstrap/dist/css/bootstrap.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
